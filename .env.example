# NotebookLM MCP Server Configuration
NOTEBOOKLM_NOTEBOOK_ID=your-actual-notebook-id-here
NOTEBOOKLM_HEADLESS=true
NOTEBOOKLM_DEBUG=false
NOTEBOOKLM_TIMEOUT=60
NOTEBOOKLM_PROFILE_DIR=./chrome_profile_notebooklm
NOTEBOOKLM_PERSISTENT_SESSION=true
NOTEBOOKLM_STREAMING_TIMEOUT=60
NOTEBOOKLM_RESPONSE_STABILITY_CHECKS=3
NOTEBOOKLM_RETRY_ATTEMPTS=3

# Optional: Base URL (default is https://notebooklm.google.com)
# NOTEBOOKLM_BASE_URL=https://notebooklm.google.com

# Optional: Custom user agent
# NOTEBOOKLM_USER_AGENT=Mozilla/5.0 (compatible; NotebookLM-MCP)
